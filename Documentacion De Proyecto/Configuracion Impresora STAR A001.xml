<?xml version="1.0"?>
<msw xmlns="http://www.Star Micronics.co.jp/psa/msw/2009/">
  <!--El elemento de configuración Esquema XML de configuración de la impresora
tiene los siguientes atributos;
 banco           : número de banco de configuración de la impresora (interruptor de memoria)/número hexadecimal (0 a F)
 Grupo de bits         : grupo de bits de configuración de la impresora (interruptor de memoria) que desea cambiar/número hexadecimal de 4 dígitos
 Opción seleccionada: grupo de bits de la opción que desea seleccionar/número hexadecimal de 4 dígitos
 Título          : [opción para facilitar la lectura] título de la configuración de la impresora/cadena

Valor de ajuste   : [opción para facilitar la lectura] título de la opción seleccionada que desea/cadena-->
  <product name="BSC10" emulation="ESCPOS" interface="USB" />
  <setting bank="2" bitset="0800" selectedoption="0000" title="Ajuste del código de barras de ladder">Desactivar</setting>
  <setting bank="4" bitset="1000" selectedoption="0000" title="Ajuste gráfico">Modo 180DPI (el espaciado predeterminado es aproximadamente 3,75 mm)</setting>
  <setting bank="0" bitset="0010" selectedoption="0000" title="Carácter asiático (DBCS)">Grupo de caracteres de un byte (SBCS)</setting>
  <setting bank="0" bitset="0300" selectedoption="0200" title="Código carácter multibyte">Chino tradicional (BIG5)</setting>
  <setting bank="3" bitset="0002" selectedoption="0000" title="Código CR">Desactivado</setting>
  <setting bank="8" bitset="F000" selectedoption="0000" title="Compresión (ancho de papel)">80 mm (sin compresión)</setting>
  <setting bank="8" bitset="0080" selectedoption="0000" title="Compresión (código de barras)">Comprimir</setting>
  <setting bank="8" bitset="0F00" selectedoption="0000" title="Compresión (longitud de papel)">Original</setting>
  <setting bank="7" bitset="0010" selectedoption="0000" title="Condiciones para BUSY">Búfer de recepción lleno o desconectado</setting>
  <setting bank="5" bitset="0600" selectedoption="0000" title="Control de paridad (serie)">No</setting>
  <setting bank="2" bitset="0003" selectedoption="0000" title="Densidad de impresión">Estándar</setting>
  <setting bank="3" bitset="00C0" selectedoption="0000" title="Espaciado entre caracteres ANK">Sin espacios</setting>
  <setting bank="7" bitset="1000" selectedoption="0000" title="Estado ASB">USB, Ethernet : Activar  / serie, paralelo: Desactivar</setting>
  <setting bank="5" bitset="0100" selectedoption="0000" title="Longitud de datos (serie)" />
  <setting bank="1" bitset="0200" selectedoption="0000" title="Margen superior">11 mm</setting>
  <setting bank="8" bitset="0004" selectedoption="0004" title="Reducción de la longitud vertical">Activar</setting>
  <setting bank="8" bitset="0008" selectedoption="0008" title="Reducción del ancho de fuente">Activar</setting>
  <setting bank="C" bitset="0200" selectedoption="0000" title="Reiniciar (paralelo) con señal INIT">Activar</setting>
  <setting bank="2" bitset="0080" selectedoption="0000" title="Rendimiento de la impresora">Calidad de impresión</setting>
  <setting bank="F" bitset="F000" selectedoption="0000" title="Sustitución de &lt;LF&gt; consecutivos">Desactivar</setting>
  <setting bank="4" bitset="0017" selectedoption="0013" title="Tamaño del papel">58mm x Receipt (33 Columns)</setting>
  <setting bank="2" bitset="0400" selectedoption="0000" title="Temporización de inicio para impresión en bloque (desencadenante)">Detectar final de Line (todo I/F)</setting>
  <setting bank="1" bitset="0080" selectedoption="0000" title="Tipo de fuente (ESC/POS)">Fuente grande</setting>
  <setting bank="2" bitset="0030" selectedoption="0000" title="Velocidad de impresión">Velocidad alta</setting>
  <setting bank="5" bitset="0007" selectedoption="0000" title="Velocidad de transmisión (serie)" />
</msw>